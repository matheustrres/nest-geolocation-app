{
	"name": "nest-geolocation-ms",
	"version": "1.0.0",
	"author": "Matheus Torres <matheustrres.rj@gmail.com>",
	"description": "A geolocation microservice built with Nestjs",
	"private": true,
	"license": "MIT",
	"scripts": {
		"docker:up": "docker-compose up -d",
		"docker:stop": "docker-compose -f docker-compose.yml down --remove-orphans",
		"prebuild": "rm -rf dist",
		"build": "nest build -p ./tsconfig.build.json -c ./nest-cli.json -b swc",
		"postbuild": "find dist -name '*.spec.js' -o -name '__tests__' | xargs rm -rf",
		"lint": "eslint \"{src,apps,libs,tests}/**/*.ts\" --fix",
		"format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
		"prestart": "pnpm build",
		"start": "dotenv -e .env.dev -- nest start",
		"start:dev": "dotenv -e .env.dev -- nest start --watch",
		"start:prod": "node --env-file=.env ./dist/main",
		"pretest": "pnpm prebuild",
		"test": "dotenv -e .env.test -- jest",
		"test:watch": "pnpm test -- --watch",
		"test:cov": "pnpm test -- --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"pretest:e2e": "pnpm docker:up database --force-recreate",
		"test:e2e": "pnpm test -- --config ./jest.e2e-config.ts",
		"posttest:e2e": "pnpm docker:stop database",
		"prepare": "husky install"
	},
	"dependencies": {
		"@nestjs/axios": "^3.0.3",
		"@nestjs/common": "^10.0.0",
		"@nestjs/config": "^3.2.3",
		"@nestjs/core": "^10.0.0",
		"@nestjs/mongoose": "^10.0.10",
		"@nestjs/platform-express": "^10.0.0",
		"@nestjs/swagger": "^8.1.0",
		"@nestjs/terminus": "^10.2.3",
		"@scalar/nestjs-api-reference": "^0.3.174",
		"@sentry/nestjs": "^8.27.0",
		"@sentry/node": "^7.119.0",
		"@sentry/profiling-node": "^8.27.0",
		"@sentry/types": "^8.27.0",
		"axios": "^1.7.5",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.14.1",
		"mongoose": "^8.5.2",
		"reflect-metadata": "^0.2.0",
		"rxjs": "^7.8.1"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.4.1",
		"@nestjs/cli": "^10.0.0",
		"@nestjs/schematics": "^10.0.0",
		"@nestjs/testing": "^10.0.0",
		"@swc/cli": "^0.3.14",
		"@swc/core": "^1.7.10",
		"@swc/helpers": "^0.5.8",
		"@swc/jest": "^0.2.36",
		"@tsconfig/node-lts": "^20.1.3",
		"@types/express": "^4.17.17",
		"@types/jest": "^29.5.2",
		"@types/node": "^20.3.1",
		"@types/supertest": "^6.0.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"dotenv-cli": "^7.4.2",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-import-helpers": "^1.3.1",
		"eslint-plugin-prettier": "^5.0.0",
		"husky": "^8.0.0",
		"jest": "^29.5.0",
		"lint-staged": "^15.2.11",
		"prettier": "^3.0.0",
		"source-map-support": "^0.5.21",
		"supertest": "^7.0.0",
		"ts-jest": "^29.1.0",
		"ts-loader": "^9.4.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.1.3"
	}
}